{
  "name": "denote-app",
  "version": "0.1.0",
  "description": "Note taking app for the STEM students.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.denote.app",
    "productName": "Denote",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.education",
      "target": "dmg",
      "icon": "assets/Slant_Icon_1024x1024.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "assets/Slant_Icon_1024x1024.ico"
    }
  },
  "repository": "https://github.com/ouragan84/slant-app",
  "publish": {
    "provider": "github",
    "owner": "ouragan84",
    "repo": "denote-app",
    "releaseType": "release"
  },
  "keywords": [
    "editorjs",
    "slant",
    "note-taking",
    "app",
    "stem",
    "students",
    "education",
    "svg-drawing",
    "excalidraw"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@editorjs/code": "^2.8.0",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/editorjs": "^2.26.5",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/paragraph": "^2.9.0",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/raw": "^2.4.0",
    "@editorjs/simple-image": "^1.5.1",
    "@editorjs/table": "^2.2.1",
    "@editorjs/warning": "^1.3.0",
    "@emotion/css": "^11.11.0",
    "css-loader": "^6.7.3",
    "electron-updater": "^5.3.0",
    "jsdom": "^21.1.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "quill": "^1.3.7",
    "quill-image-drop-module": "^1.0.3",
    "quill-image-resize-module-react": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-editor-js": "^2.1.0",
    "react-quill": "^2.0.0",
    "sass": "^1.62.1",
    "slate": "^0.94.0",
    "slate-history": "^0.93.0",
    "slate-react": "^0.94.0",
    "style-loader": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@editorjs/checklist": "^1.5.0",
    "babel-loader": "^9.1.2",
    "electron": "^24.1.3",
    "electron-builder": "^24.3.0",
    "electron-reload": "^2.0.0-alpha.1",
    "sass-loader": "^13.2.2",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  }
}
