{
  "name": "denote-app",
  "version": "0.1.0",
  "description": "Note taking app for the STEM students.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "watch": "webpack --mode production --config webpack.common.js --watch",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.denote.app",
    "productName": "Denote",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.education",
      "target": "dmg",
      "icon": "assets/Slant_Icon_1024x1024.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "assets/Slant_Icon_1024x1024.ico"
    }
  },
  "repository": "https://github.com/ouragan84/slant-app",
  "publish": {
    "provider": "github",
    "owner": "ouragan84",
    "repo": "denote-app",
    "releaseType": "release"
  },
  "keywords": [
    "editorjs",
    "slant",
    "note-taking",
    "app",
    "stem",
    "students",
    "education",
    "svg-drawing",
    "excalidraw"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^37.1.0",
    "@ckeditor/ckeditor5-build-classic": "^37.1.0",
    "@ckeditor/ckeditor5-core": "^37.1.0",
    "@ckeditor/ckeditor5-editor-classic": "^37.1.0",
    "@ckeditor/ckeditor5-essentials": "^37.1.0",
    "@ckeditor/ckeditor5-paragraph": "^37.1.0",
    "@ckeditor/ckeditor5-react": "^6.0.0",
    "css-loader": "^6.7.3",
    "electron-updater": "^5.3.0",
    "jsdom": "^21.1.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons-kit": "^2.0.0",
    "react-latex-next": "^2.2.0",
    "react-mathquill": "^1.0.3",
    "react-sketch": "^0.5.1",
    "sass": "^1.62.1",
    "slate": "^0.34.5",
    "slate-react": "^0.13.2",
    "style-loader": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "electron": "^24.1.3",
    "electron-builder": "^24.3.0",
    "electron-reload": "^2.0.0-alpha.1",
    "sass-loader": "^13.2.2",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  }
}
